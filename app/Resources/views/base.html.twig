        <!-- Global site tag (gtag.js) - Google Analytics -->
        <script async src="https://www.googletagmanager.com/gtag/js?id=UA-131757964-1"></script>
        <script>
          window.dataLayer = window.dataLayer || [];
          function gtag(){dataLayer.push(arguments);}
          gtag('js', new Date());

          gtag('config', 'UA-131757964-1');
        </script>
        {% block meta %}
          <meta charset="UTF-8" />
          <meta http-equiv="X-UA-Compatible" content="IE=edge">
          <meta name="description" content="{{ app_name }} - {{ 'app.phrase'|trans }}">
          <meta name="author" content="{{ app_name }}">
          <meta name="viewport" content="width=device-width, initial-scale=1">
        {% endblock %}
        {% block head_inner %}
          <title>
            {% block title %}
              {{ app_name }} - {{ 'app.phrase'|trans }}
            {% endblock %}
          </title>
          {% block stylesheets %}{% endblock %}
          {% block head_scripts %}{% endblock %}
          <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        {% endblock %}
      </head>
    {% endblock %}
    {% block body %}
      <body {% block body_style %}{% endblock %}>
