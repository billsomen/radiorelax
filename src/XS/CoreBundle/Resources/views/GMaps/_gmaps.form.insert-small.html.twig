<div class="col-md-12" style="display: none">
  {#{{ dump() }}#}
</div>
{% set target_id = form_element.vars.id %}

<input
  class="form-control autocomplete_gmaps form-group"
  id="{{ target_id }}"
  placeholder="Ville"
  required="required"
  style="background-color: transparent"
  onFocus="geolocate('{{ target_id }}')"
  {% if update_phone is defined %}
    {% if  update_phone %}
      onchange="initCodeUpdate()"
    {% endif %}
  {% endif %}
  value="{{ form_element.formattedAddress.vars.value }}"
  type="text"
/>

<div class="col-md-6" style="display: none">
  {{ form_widget(form_element.formattedAddress, {'id': target_id~''~'formatted_address'}) }}
</div>
<table class="table table-light table-hover" style="display: none">
  <tr>
    <td>Saisir l'Adresse</td>
    <td colspan="2">

    </td>
  </tr>
  <tr>
    <td>Addresse Finale</td>
    <td colspan="2">

    </td>
  </tr>
  <tr >
    <td>Lat</td>
    <td colspan="2">
      {{ form_widget(form_element.lat, {'id': target_id~''~'lat'}) }}
    </td>
  </tr>
  <tr style="display: none">
    <td>Lng</td>
    <td colspan="2">
      {{ form_widget(form_element.lng, {'id': target_id~''~'lng'}) }}
    </td>
  </tr>

  <tr style="display: none">
    <td>Place Id</td>
